{
    "openapi": "3.0.0",
    "info": {
        "title": "DNS API",
        "description": "This is a sample server for a DNS store.",
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "api.example.com/v1",
            "description": "Development server"
        }
    ],
    "paths": {
        "/data/dns/{id}": {
            "get": {
                "tags": [
                    "dns"
                ],
                "description": "Returns ID based DNS ",
                "summary": "Find DNS by ID",
                "operationId": "getDnsById",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "ID of DNS",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "invalid request",
                        "content": {}
                    },
                    "404": {
                        "description": "ID not found",
                        "content": {}
                    }
                }
            },
            "delete": {
                "tags": [
                    "dns"
                ],
                "summary": "Delete DNS by ID",
                "description": "Delete DNS",
                "operationId": "deleteDnsById",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "ID of DNS to deleted",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "items": {
                                "type": "string"
                            }
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "successful eraser!"
                    },
                    "400": {
                        "description": "Invalid ID",
                        "content": {}
                    },
                    "404": {
                        "description": "ID not found",
                        "content": {}
                    }
                }
            },
            "post": {
                "tags": [
                    "dns"
                ],                
                "description": "creating DNS",
                "summary": "Cretate DNS",
                "operationId": "postDns",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "required": [
                                    "name",
                                    "ipAddress",
                                    "domainName"
                                ],
                                "properties": {
                                    "name": {
                                        "type": "string"
                                    },
                                    "ipAddress": {
                                        "type": "string"
                                    },
                                    "domainName": {
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Created DNS.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    },
                    "405": {
                        "description": "Method Not Allowed",
                        "content": {}
                    }
                }
            },
            "put": {
                "tags": [
                    "dns"
                ],
                "description": "Updated dns information",
                "summary": "update of the DNS.",
                "operationId": "updateDnsbyId",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "DNS ID to be updated",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "items": {
                                "type": "string"
                            }
                        }
                    }
                ],
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "required": [
                                    "name",
                                    "ipAddress",
                                    "domainName"
                                ],
                                "properties": {
                                    "name": {
                                        "type": "string"
                                    },
                                    "ipAddress": {
                                        "type": "string"
                                    },
                                    "domainName": {
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "update of the DNS.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Invalid ID",
                        "content": {}
                    },
                    "404": {
                        "description": "ID not found",
                        "content": {}
                    }
                }
            }
        }
    }
}
